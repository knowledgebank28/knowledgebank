  }
  .menu-toggle ul a,
  .main-navigation.toggled .nav-menu ul a {
    color: #666666;
    width: 100%;
  }
  .menu-toggle ul a:hover,
  .main-navigation.toggled .nav-menu ul a:hover {
    color: #07a1f2;
  }

  .main-navigation ul {
    display: none;
  }

  .menu-toggle {
    clear: none;
  }
