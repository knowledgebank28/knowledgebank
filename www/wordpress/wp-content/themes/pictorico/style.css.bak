    clear: both;
    display: block;
    float: none;
    opacity: 1;
    visibility: visible;
    margin: 0;
    position: relative;
    top: auto;
    left: auto;
  }
  .menu-toggle ul a,
  .main-navigation.toggled .nav-menu ul a {
    padding-left: 30px;
  }
  .menu-toggle ul ul,
  .main-navigation.toggled .nav-menu ul ul {
    clear: both;
    float: none;
    left: auto;
    top: auto;
    width: 100%;
  }
  .menu-toggle ul ul a,
  .main-navigation.toggled .nav-menu ul ul a {
    padding-left: 45px;
  }
  .menu-toggle ul ul ul a,
  .main-navigation.toggled .nav-menu ul ul ul a {
